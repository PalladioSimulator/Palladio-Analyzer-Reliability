<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.uka.ipd.sdq.sensitivity.presentation.SensitivityModelWizardID"
            name="%_UI_SensitivityModelWizard_label"
            class="de.uka.ipd.sdq.sensitivity.presentation.SensitivityModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SensitivityModelFile.gif">
         <description>%_UI_SensitivityModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="de.uka.ipd.sdq.sensitivity.presentation.SensitivityEditorID"
            name="%_UI_SensitivityEditor_label"
            icon="icons/full/obj16/SensitivityModelFile.gif"
            extensions="sensitivity"
            class="de.uka.ipd.sdq.sensitivity.presentation.SensitivityEditor"
            contributorClass="de.uka.ipd.sdq.sensitivity.presentation.SensitivityActionBarContributor">
      </editor>
   </extension>

</plugin>
